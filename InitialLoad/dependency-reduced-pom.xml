<?xml version="1.0" encoding="UTF-8"?>
<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/maven-v4_0_0.xsd">
  <modelVersion>4.0.0</modelVersion>
  <groupId>com.gigaspaces</groupId>
  <artifactId>InitialLoad</artifactId>
  <name>InitialLoad</name>
  <version>1.0-SNAPSHOT</version>
  <url>http://maven.apache.org</url>
  <build>
    <finalName>initial-loader</finalName>
    <plugins>
      <plugin>
        <artifactId>maven-shade-plugin</artifactId>
        <version>2.2</version>
        <executions>
          <execution>
            <phase>package</phase>
            <goals>
              <goal>shade</goal>
            </goals>
            <configuration />
          </execution>
        </executions>
      </plugin>
      <plugin>
        <artifactId>maven-compiler-plugin</artifactId>
        <configuration>
          <source>7</source>
          <target>7</target>
        </configuration>
      </plugin>
    </plugins>
  </build>
  <repositories>
    <repository>
      <id>org.openspaces</id>
      <url>http://maven-repository.openspaces.org</url>
    </repository>
    <repository>
      <id>ibm</id>
      <url>https://artifacts.alfresco.com/nexus/content/repositories/public</url>
    </repository>
  </repositories>
  <dependencies>
    <dependency>
      <groupId>org.gigaspaces</groupId>
      <artifactId>xap-datagrid</artifactId>
      <version>12.2.0</version>
      <scope>provided</scope>
      <exclusions>
        <exclusion>
          <artifactId>xap-common</artifactId>
          <groupId>org.gigaspaces</groupId>
        </exclusion>
        <exclusion>
          <artifactId>xap-asm</artifactId>
          <groupId>org.gigaspaces</groupId>
        </exclusion>
        <exclusion>
          <artifactId>xap-trove</artifactId>
          <groupId>org.gigaspaces</groupId>
        </exclusion>
      </exclusions>
    </dependency>
  </dependencies>
  <properties>
    <skipTests>true</skipTests>
    <logback.version>1.0.13</logback.version>
    <slf4j.version>1.7.5</slf4j.version>
    <gsVersion>12.2.0</gsVersion>
    <project.build.sourceEncoding>UTF-8</project.build.sourceEncoding>
    <gsType>PU</gsType>
    <xap.version>12.2.0</xap.version>
  </properties>
</project>

